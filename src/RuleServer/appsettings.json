{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RuleAlertDatabase": "Data source = ruleAlert.db"
  },
  "RuleService": {
    "RuleSet": [
      {
        "RuleName": "rule name",
        "ConditionExpression": "sensorId == \"0x20\" && temperature > 10.0 && power < 12.0",
        "LogThresholdForTimes": 5
      }
    ]
  }
}
